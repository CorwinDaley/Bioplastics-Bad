<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/activity_home">

    <activity
        android:id="@+id/activity_home"
        android:name="com.example.bioplasticbad.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/login"
        android:name="com.example.bioplasticbad.homepage"
        android:label="fragment_homepage"
        tools:layout="@layout/fragment_homepage" >
        <action
            android:id="@+id/action_homepage_to_login"
            app:destination="@id/activity_home" />
        <action
            android:id="@+id/action_login_to_blankFragment"
            app:destination="@id/homePage" />
    </fragment>
    <fragment
        android:id="@+id/profilepage"
        android:name="com.example.bioplasticbad.profilepage"
        android:label="fragment_profilepage"
        tools:layout="@layout/fragment_profilepage" >
        <action
            android:id="@+id/action_profilepage_to_homepage"
            app:destination="@id/activity_home" />
        <action
            android:id="@+id/action_profilepage_to_blankFragment"
            app:destination="@id/homePage" />
    </fragment>
    <fragment
        android:id="@+id/homePage"
        android:name="com.example.bioplasticbad.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" >
        <action
            android:id="@+id/action_blankFragment_to_profilepage"
            app:destination="@id/profilepage" />
        <action
            android:id="@+id/action_blankFragment_to_login"
            app:destination="@id/login" />
    </fragment>
</navigation>